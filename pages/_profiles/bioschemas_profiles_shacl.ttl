        [ sh:description "Link to social media websites like twitter or facebook." ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/socialMedia> ],
        [ sh:description "A relationship between two organizations where the first includes the second, e.g., as a subsidiary. See also: the more specific 'department' property. Inverse property: parentOrganization." ;
            sh:path [ sh:alternativePath ( <http://schema.org/subOrganization> schema:subOrganization ) ] ],
        [ sh:description "The name of the item." ;
