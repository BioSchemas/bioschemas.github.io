@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://bioschemas.org/profiles/ChemicalSubstance/0.4-RELEASE> a sh:NodeShape ;
    sh:description "This profile describes a ChemicalSubstance which is 'a portion of matter of constant composition, composed of molecular entities of the same type or of different types' (source: ChEBI:59999). Note, the parent class for this profile has been updated to a pending class in schema.org." ;
    sh:property [ sh:description "The name of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/name> schema:name ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "This is used to state the Bioschemas profile that the markup relates to. The identifier can be the url for the version of this bioschemas class on github: https://github.com/BioSchemas/specifications/blob/master/ChemicalSubstance/jsonld/ChemicalSubstance_v0.4-RELEASE.json" ;
            sh:minCount 1 ;
            sh:path dcterms:conformsTo ;
            sh:severity sh:Violation ],
        [ sh:description "The chemical composition describes the identity and relative ratio of the chemical elements that make up the substance. For substances this often cannot be accurately determined, an approximation is acceptable." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/chemicalComposition> schema:chemicalComposition ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Indicates a BioChemEntity that (in some sense) has this BioChemEntity as a part. Inverse property: isPartOfBioChemEntity" ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/hasBioChemEntityPart> schema:hasBioChemEntityPart ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "An alias for the item." ;
            sh:path [ sh:alternativePath ( <http://schema.org/alternateName> schema:alternateName ) ] ],
        [ sh:description "A BioChemEntity that is know to interact with this item." ;
            sh:path [ sh:alternativePath ( <http://schema.org/bioChemInteraction> schema:bioChemInteraction ) ] ],
        [ sh:description "A similar molecular entity, e.g., obtained by fingerprint similarity algorithm." ;
            sh:path [ sh:alternativePath ( <http://schema.org/bioChemSimilarity> schema:bioChemSimilarity ) ] ],
        [ sh:description "A role played by the molecular entity within a biological context." ;
            sh:path [ sh:alternativePath ( <http://schema.org/biologicalRole> schema:biologicalRole ) ] ],
        [ sh:description "A role played by the BioChemEntity or molecular entity within a chemical context." ;
            sh:path [ sh:alternativePath ( <http://schema.org/chemicalRole> schema:chemicalRole ) ] ],
        [ sh:description "A description of the item." ;
            sh:path [ sh:alternativePath ( <http://schema.org/description> schema:description ) ] ],
        [ sh:description "A sub property of description. A short description of the item used to disambiguate from other, similar items. Information from other properties (in particular, name) may be necessary for the description to be useful for disambiguation." ;
            sh:path [ sh:alternativePath ( <http://schema.org/disambiguatingDescription> schema:disambiguatingDescription ) ] ],
        [ sh:description "The identifier property represents any kind of identifier for any kind of <a class=\"localLink\" href=\"http://schema.org/Thing\">Thing</a>, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See <a href=\"/docs/datamodel.html#identifierBg\">background notes</a> for more details." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/identifier> schema:identifier ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "An image of the item. This can be a <a class=\"localLink\" href=\"http://schema.org/URL\">URL</a> or a fully described <a class=\"localLink\" href=\"http://schema.org/ImageObject\">ImageObject</a>." ;
            sh:path [ sh:alternativePath ( <http://schema.org/image> schema:image ) ] ],
        [ sh:description "Intended use of the BioChemEntity by humans." ;
            sh:path [ sh:alternativePath ( <http://schema.org/potentialUse> schema:potentialUse ) ] ],
        [ sh:description "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website." ;
            sh:path [ sh:alternativePath ( <http://schema.org/sameAs> schema:sameAs ) ] ],
        [ sh:description "URL of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/url> schema:url ) ] ;
            sh:severity sh:Violation ] ;
    sh:targetClass <http://schema.org/ChemicalSubstance>,
        schema:ChemicalSubstance .

<https://bioschemas.org/profiles/ComputationalTool/1.0-RELEASE> a sh:NodeShape ;
    sh:description "The Life Science Tools specification provides a way to describe bioscience tools and software on the World Wide Web. It defines a set of metadata and vocabularies, built on top of existing technologies and standards, that can be used to represent such tools in Web pages and applications. The goal of the specification is to make it easier to discover. Version 1.0-RELEASE.<h3>Summary of Changes</h3> <p>Changes since the 0.6-DRAFT</p> <ul> <li>additionalType property deprecated and replaced by applicationCategory.</li><li>example added for the name property.</li></ul>" ;
    sh:property [ sh:description "A short description of the tool." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/description> schema:description ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "A small image representing the tool, such as an icon. A thumbnail image relevant to the Thing." ;
            sh:path [ sh:alternativePath ( <http://schema.org/thumbnailUrl> schema:thumbnailUrl ) ] ],
        [ sh:description "This is used to state the Bioschemas profile that the markup relates to. The identifier can be the url for the version of this bioschemas class on github: https://github.com/BioSchemas/specifications/blob/master/ComputationalTool/jsonld/ComputationalTool_v1.0-RELEASE.json" ;
            sh:minCount 1 ;
            sh:path dcterms:conformsTo ;
            sh:severity sh:Violation ],
        [ sh:description "Type of software application, e.g. 'Game, Multimedia'. Please use terms from the 'Tool type' table in the biotools documentation." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/applicationCategory> schema:applicationCategory ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Subcategory of the application, e.g. 'Arcade Game'. Use an EDAM:Topic to describe the category of application" ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/applicationSubCategory> schema:applicationSubCategory ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "The author of this content or rating. Please note that author is special in that HTML 5 provides a special mechanism for indicating authorship via the rel tag. That is equivalent to this and may be used interchangeably." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/author> schema:author ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Publication about this tool. A citation or reference to another creative work, such as another publication, web page, scholarly article, etc." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/citation> schema:citation ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Features or modules provided by this application (and possibly required by other applications). Functionality provided by the tool. Note: Bioschemas have removed Text from the Expected Types." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/featureList> schema:featureList ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "The license of a software, curated dataset, etc. A license document that applies to this content, typically indicated by URL. Use SPDX list of licenses in form https://spdx.org/licenses/" ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/license> schema:license ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Version(s) of the tool, which this information is valid for. Can also be a comma-delimited list and include hyphen-separated ranges of versions." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/softwareVersion> schema:softwareVersion ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "A suite of tools, to which the tool belongs. The name of the application suite to which the application belongs (e.g. Excel belongs to Office)." ;
            sh:path [ sh:alternativePath ( <http://schema.org/applicationSuite> schema:applicationSuite ) ] ],
        [ sh:description "Link to the source code repository of the tool." ;
            sh:path <https://discovery.biothings.io/view/bioschemas/codeRepository> ],
        [ sh:description "A secondary contributor to the CreativeWork or Event." ;
            sh:path [ sh:alternativePath ( <http://schema.org/contributor> schema:contributor ) ] ],
        [ sh:description "The name of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/name> schema:name ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "A link to the page containing the comments of the CreativeWork." ;
            sh:path [ sh:alternativePath ( <http://schema.org/discussionUrl> schema:discussionUrl ) ] ],
        [ sh:description "Link(s) to the downloadable(s)." ;
            sh:path [ sh:alternativePath ( <http://schema.org/downloadUrl> schema:downloadUrl ) ] ],
        [ sh:description "A person or organization that supports (sponsors) something through some kind of financial contribution." ;
            sh:path [ sh:alternativePath ( <http://schema.org/funder> schema:funder ) ] ],
        [ sh:description "Indicates any software dependency. Indicates an item or CreativeWork that is part of this item, or CreativeWork (in some sense)." ;
            sh:path [ sh:alternativePath ( <http://schema.org/hasPart> schema:hasPart ) ] ],
        [ sh:description "The identifier property represents any kind of identifier for any kind of <a class=\"localLink\" href=\"http://schema.org/Thing\">Thing</a>, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See <a href=\"/docs/datamodel.html#identifierBg\">background notes</a> for more details." ;
            sh:path [ sh:alternativePath ( <http://schema.org/identifier> schema:identifier ) ] ],
        [ sh:description "Specification of a consumed input." ;
            sh:path <https://discovery.biothings.io/view/bioschemas/input> ],
        [ sh:description "A flag to signal that the tool or the service are available to be used for free." ;
            sh:path [ sh:alternativePath ( <http://schema.org/isAccessibleForFree> schema:isAccessibleForFree ) ] ],
        [ sh:description "A tool or another work which this tool is based on, or is an extension, distribution, or deployment of. A resource from which this work is derived or from which it is a modification or adaption." ;
            sh:path <https://discovery.biothings.io/view/bioschemas/isBasedOn> ],
        [ sh:description "A collection, other than a suite of tools, to which the tool belongs.. Indicates an item or CreativeWork that this item, or CreativeWork (in some sense), is part of." ;
            sh:path [ sh:alternativePath ( <http://schema.org/isPartOf> schema:isPartOf ) ] ],
        [ sh:description "Additional keywords or tags important for this tool. Multiple entries are delimited by commas. Keywords or tags used to describe this content. Multiple entries in a keywords list are typically delimited by commas." ;
            sh:path [ sh:alternativePath ( <http://schema.org/keywords> schema:keywords ) ] ],
        [ sh:description "Operating systems on which the tool can be used (without additional wrapping). Operating systems supported (Windows 7, OSX 10.6, Android 1.6). Please use terms from the ‘Operating systems’ table in the biotools documentation" ;
            sh:path [ sh:alternativePath ( <http://schema.org/operatingSystem> schema:operatingSystem ) ] ],
        [ sh:description "Specification of a produced output." ;
            sh:path <https://discovery.biothings.io/view/bioschemas/output> ],
        [ sh:description "Homepage of the tool. URL of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/url> schema:url ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "The main programming language(s) used to build or execute the tool. Please use terms from the ‘Programming language’ table in the Bio.Tools documentation" ;
            sh:path <https://discovery.biothings.io/view/bioschemas/programmingLanguage> ],
        [ sh:description "The provider organization of a web application, database portal, web service, etc. Note: Bioschemas have removed Person from the Expected Types. The service provider, service operator, or service performer; the goods producer. Another party (a seller) may offer those services or goods on behalf of the provider. A provider may also serve as the seller." ;
            sh:path [ sh:alternativePath ( <http://schema.org/provider> schema:provider ) ] ],
        [ sh:description "Additional sub-tools (add-ons, plug-ins, tools in a suite, etc.) that are included in the tool or workflow. Additional content for a software application." ;
            sh:path [ sh:alternativePath ( <http://schema.org/softwareAddOn> schema:softwareAddOn ) ] ],
        [ sh:description "A documentation of the tool." ;
            sh:path [ sh:alternativePath ( <http://schema.org/softwareHelp> schema:softwareHelp ) ] ] ;
    sh:targetClass <http://schema.org/SoftwareApplication>,
        schema:SoftwareApplication .

<https://bioschemas.org/profiles/ComputationalWorkflow/1.0-RELEASE> a sh:NodeShape ;
    sh:description "A computational workflow consists of an orchestrated and repeatable pattern of activity enabled by the systematic organization of resources into processes that transform materials, provide services, or process information (source Wikipedia.org). It is executed by a computational process and is thus distinct from laboratory or business workflows. version 1.0-RELEASE" ;
    sh:property [ sh:description "An input required to use the computational workflow (eg. Excel spreadsheet, BAM file)" ;
            sh:minCount 1 ;
            sh:path <https://discovery.biothings.io/view/bioschemas/input> ;
            sh:severity sh:Violation ],
        [ sh:description "Date of first broadcast/publication." ;
            sh:path [ sh:alternativePath ( <http://schema.org/datePublished> schema:datePublished ) ] ],
        [ sh:description "Media type typically expressed using a MIME format (see <a href=\"http://www.iana.org/assignments/media-types/media-types.xhtml\">IANA site</a> and <a href=\"https://developer.mozilla.org/en-US/docs/Web/HTTP/Basics_of_HTTP/MIME_types\">MDN reference</a>) e.g. application/zip for a SoftwareApplication binary, audio/mpeg for .mp3 etc.).<br/><br/> In cases where a <a class=\"localLink\" href=\"http://schema.org/CreativeWork\">CreativeWork</a> has several media type representations, <a class=\"localLink\" href=\"http://schema.org/encoding\">encoding</a> can be used to indicate each <a class=\"localLink\" href=\"http://schema.org/MediaObject\">MediaObject</a> alongside particular <a class=\"localLink\" href=\"http://schema.org/encodingFormat\">encodingFormat</a> information.<br/><br/> Unregistered or niche encoding and file formats can be indicated instead via the most appropriate URL, e.g. defining Web page or a Wikipedia/Wikidata entry." ;
            sh:path [ sh:alternativePath ( <http://schema.org/encodingFormat> schema:encodingFormat ) ] ],
        [ sh:description "The identifier property represents any kind of identifier for any kind of <a class=\"localLink\" href=\"http://schema.org/Thing\">Thing</a>, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See <a href=\"/docs/datamodel.html#identifierBg\">background notes</a> for more details." ;
            sh:path [ sh:alternativePath ( <http://schema.org/identifier> schema:identifier ) ] ],
        [ sh:description "An image of the item. This can be a <a class=\"localLink\" href=\"http://schema.org/URL\">URL</a> or a fully described <a class=\"localLink\" href=\"http://schema.org/ImageObject\">ImageObject</a>." ;
            sh:path [ sh:alternativePath ( <http://schema.org/image> schema:image ) ] ],
        [ sh:description "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/creator> schema:creator ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "The date on which the CreativeWork was created or the item was added to a DataFeed." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/dateCreated> schema:dateCreated ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "An output produced by the workflow" ;
            sh:minCount 1 ;
            sh:path <https://discovery.biothings.io/view/bioschemas/output> ;
            sh:severity sh:Violation ],
        [ sh:description "A license document that applies to this content, typically indicated by URL." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/license> schema:license ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "Indicates the party responsible for generating and publishing the current structured data markup, typically in cases where the structured data is derived automatically from existing published content but published on a different site. For example, student projects and open data initiatives often re-publish existing content with more explicitly structured metadata. The <a class=\"localLink\" href=\"http://schema.org/sdPublisher\">sdPublisher</a> property helps make such practices more explicit." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/sdPublisher> schema:sdPublisher ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "The version of the CreativeWork embodied by a specified resource." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/version> schema:version ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "The computer programming language." ;
            sh:minCount 1 ;
            sh:path <https://discovery.biothings.io/view/bioschemas/programmingLanguage> ;
            sh:severity sh:Violation ],
        [ sh:description "This is used to state the Bioschemas profile that the markup relates to. The identifier can be the url for the version of this bioschemas class on github: https://github.com/BioSchemas/specifications/blob/master/ComputationalWorkflow/jsonld/ComputationalWorkflow_v1.0-RELEASE.json" ;
            sh:minCount 1 ;
            sh:path dcterms:conformsTo ;
            sh:severity sh:Violation ],
        [ sh:description "A citation or reference to another creative work, such as another publication, web page, scholarly article, etc." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/citation> schema:citation ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "A secondary contributor to the CreativeWork or Event." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/contributor> schema:contributor ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "The name of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/name> schema:name ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "The status of a creative work in terms of its stage in a lifecycle. Example terms include Incomplete, Draft, Published, Obsolete. Some organizations define a set of terms for the stages of their publication lifecycle." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/creativeWorkStatus> schema:creativeWorkStatus ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "A description of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/description> schema:description ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Documentation describing the ComputationalWorkflow and its use." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/documentation> schema:documentation ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "The funding for the workflow" ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/funding> schema:funding ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Indicates an item or CreativeWork that is part of this item, or CreativeWork (in some sense)." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/hasPart> schema:hasPart ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "A resource from which this work is derived or from which it is a modification or adaption." ;
            sh:minCount 1 ;
            sh:path <https://discovery.biothings.io/view/bioschemas/isBasedOn> ;
            sh:severity sh:Warning ],
        [ sh:description "Keywords or tags used to describe this content. Multiple entries in a keywords list are typically delimited by commas." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/keywords> schema:keywords ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "maintainer of a Dataset, software package (SoftwareApplication), or other Project. A maintainer is a Person or Organization that manages contributions to, and/or publication of, some (typically complex) artifact. It is common for distributions of software and data to be based on “upstream” sources. When maintainer is applied to a specific version of something e.g. a particular version or packaging of a Dataset, it is always possible that the upstream source has a different maintainer. The isBasedOn property can be used to indicate such relationships between datasets to make the different maintenance roles clear. Similarly in the case of software, a package may have dedicated maintainers working on integration into software distributions such as Ubuntu, as well as upstream maintainers of the underlying work." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/maintainer> schema:maintainer ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "The person or organization who produced the work (e.g. music album, movie, tv/radio series etc.)." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/producer> schema:producer ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "The publisher of the creative work." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/publisher> schema:publisher ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Runtime platform or script interpreter dependencies (Example - Java v1, Python2.3, .Net Framework 3.0)." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/runtimePlatform> schema:runtimePlatform ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "URL of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/url> schema:url ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "Renaming schema.org/requirements as softwareRequirements. Component dependency requirements for application. This includes runtime environments and shared libraries that are not included in the application distribution package, but required to run the application (Examples: DirectX, Java or .NET runtime). Supersedes requirements." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/softwareRequirements> schema:softwareRequirements ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Target Operating System / Product to which the code applies. If applies to several versions, just the product name can be used." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/targetProduct> schema:targetProduct ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "An alias for the item." ;
            sh:path [ sh:alternativePath ( <http://schema.org/alternateName> schema:alternateName ) ] ],
        [ sh:description "Conditions that affect the availability of, or method(s) of access to, an item. Typically used for real world items such as an ArchiveComponent held by an ArchiveOrganization. This property is not suitable for use as a general Web access control mechanism. It is expressed only in natural language. For example “Available by appointment from the Reading Room” or “Accessible only from logged-in accounts “." ;
            sh:path [ sh:alternativePath ( <http://schema.org/conditionsOfAccess> schema:conditionsOfAccess ) ] ],
        [ sh:description "The date on which the CreativeWork was most recently modified or when the item's entry was modified within a DataFeed." ;
            sh:path [ sh:alternativePath ( <http://schema.org/dateModified> schema:dateModified ) ] ] ;
    sh:targetClass <http://schema.org/ComputationalWorkflow>,
        schema:ComputationalWorkflow .

<https://bioschemas.org/profiles/Course/1.0-RELEASE> a sh:NodeShape ;
    sh:description "Specification describing a course. This specification must be used in tandem with a courseInstance. A course is used to describe the broad, common aspects of a recurring training event - whereas a course instance is about the specific times and location of when that course is held. <h4>Summary of Changes</h4> No changes since 0.10-DRAFT of the Course profile.</li> </ul> Version: 1.0-RELEASE (13 September 2022)" ;
    sh:property [ sh:description "A description of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/description> schema:description ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "Strongly recommended if exists. A description of the qualification, award, certificate, diploma or other educational credential awarded as a consequence of successful completion of this course. A description of the qualification, award, certificate, diploma or other educational credential awarded as a consequence of successful completion of this course." ;
            sh:path [ sh:alternativePath ( <http://schema.org/educationalCredentialAwarded> schema:educationalCredentialAwarded ) ] ],
        [ sh:description "A training material used in a course such as an exercise, handouts, or slides. Inverse property: isPartOf. If this varies in a CourseInstance, use workFeatutred to override this property value. Indicates an item or CreativeWork that is part of this item, or CreativeWork (in some sense). Inverse property: isPartOf." ;
            sh:path [ sh:alternativePath ( <http://schema.org/hasPart> schema:hasPart ) ] ],
        [ sh:description "An image of the item. This can be a URL or a fully described ImageObject." ;
            sh:path [ sh:alternativePath ( <http://schema.org/image> schema:image ) ] ],
        [ sh:description "A course, book or other resource on which this Course is based. A resource that was used in the creation of this resource. This term can be repeated for multiple sources. For example, http://example.com/great-multiplication-intro.html. Supersedes isBasedOnUrl." ;
            sh:path <https://discovery.biothings.io/view/bioschemas/isBasedOn> ],
        [ sh:description "A thumbnail image relevant to the Thing." ;
            sh:path [ sh:alternativePath ( <http://schema.org/thumbnailUrl> schema:thumbnailUrl ) ] ],
        [ sh:description "This is used to state the Bioschemas profile that the markup relates to. The identifier can be the url for the version of this bioschemas class on github: https://github.com/BioSchemas/specifications/blob/master/Course/jsonld/Course_v1.0-RELEASE.json" ;
            sh:minCount 1 ;
            sh:path dcterms:conformsTo ;
            sh:severity sh:Violation ],
        [ sh:description "The subject of this Course. Use the DefinedTerm type to add a controlled vocabulary term to categorise the course (such as using the [EDAM Topic ontology](https://bioportal.bioontology.org/ontologies/EDAM/?p=classes&conceptid=http%3A%2F%2Fedamontology.org%2Ftopic_0003)). The subject matter of the content. Inverse property: subjectOf." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/about> schema:about ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "An abstract is a short description that summarizes a Course An abstract is a short description that summarizes a CreativeWork" ;
            sh:minCount 1 ;
