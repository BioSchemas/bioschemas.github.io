            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/mainEntity> schema:mainEntity ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "ScholarlyArticle where this annotation is located. It is also possible to link from the ScholarlyArticle to the SemanticTextAnnotation via about Note: For the Bioschemas case, the preferred property range is ScholarlyArticle A CreativeWork or Event about this Thing. Inverse property: about" ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/subjectOf> schema:subjectOf ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "URL of additional sources for the dataset" ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/sameAs> schema:sameAs ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "Number of times this annotation is found in the associated scholarly article. The number of comments this CreativeWork (e.g. Article, Question or Answer) has received. This is most applicable to works published in Web sites with commenting system; additional comments may exist elsewhere." ;
            sh:path [ sh:alternativePath ( <http://schema.org/commentCount> schema:commentCount ) ] ],
        [ sh:description "The creator/author of this CreativeWork. This is the same as the Author property for CreativeWork." ;
            sh:path [ sh:alternativePath ( <http://schema.org/creator> schema:creator ) ] ],
        [ sh:description "The date on which the CreativeWork was created or the item was added to a DataFeed." ;
            sh:path [ sh:alternativePath ( <http://schema.org/dateCreated> schema:dateCreated ) ] ],
        [ sh:description "The date on which the CreativeWork was most recently modified or when the item's entry was modified within a DataFeed." ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/dateModified> ],
        [ sh:description "Date of first broadcast/publication." ;
            sh:path [ sh:alternativePath ( <http://schema.org/datePublished> schema:datePublished ) ] ],
        [ sh:description "If available. The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See [background notes](http://schema.org/docs/datamodel.html#identifierBg) for more details." ;
            sh:path [ sh:alternativePath ( <http://schema.org/identifier> schema:identifier ) ] ],
        [ sh:description "The language of the content or performance or used in an action. Please use one of the language codes from the IETF BCP 47 standard. See also availableLanguage. Supersedes language." ;
            sh:path [ sh:alternativePath ( <http://schema.org/inLanguage> schema:inLanguage ) ] ],
        [ sh:description "Dataset or collection aggregating this annotation Indicates an item or CreativeWork that this item, or CreativeWork (in some sense), is part of. Inverse property: hasPart." ;
            sh:path [ sh:alternativePath ( <http://schema.org/isPartOf> schema:isPartOf ) ] ],
        [ sh:description "The position of an item in a series or sequence of items." ;
            sh:path [ sh:alternativePath ( <http://schema.org/position> schema:position ) ] ],
        [ sh:description "Expression found in the text corresponding to this annotation. The textual content of this CreativeWork." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/text> schema:text ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "This is used to state the Bioschemas profile that the markup relates to. The identifier can be the url for the version of this bioschemas class on github: https://github.com/BioSchemas/specifications/tree/master/SemanticTextAnnotation/jsonld/SemanticTextAnnotation_v0.3-DRAFT.json" ;
            sh:minCount 1 ;
            sh:path dcterms:conformsTo ;
            sh:severity sh:Violation ] ;
    sh:targetClass <http://schema.org/CreativeWork>,
        schema:CreativeWork .

<https://bioschemas.org/profiles/SequenceAnnotation/0.7-DRAFT> a sh:NodeShape ;
