        [ sh:description "Products owned by the organization or person." ;
            sh:path [ sh:alternativePath ( <http://schema.org/owns> schema:owns ) ] ],
        [ sh:description "The larger organization that this organization is a subOrganization of, if any. Supersedes branchOf. Inverse property: subOrganization." ;
