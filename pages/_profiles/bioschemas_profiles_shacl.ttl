        [ sh:description "The total number of individuals that may attend an event or venue." ;
            sh:path [ sh:alternativePath ( <http://schema.org/maximumAttendeeCapacity> schema:maximumAttendeeCapacity ) ] ],
        [ sh:description "The name of the course. Course instance name can be used to override Course name for variations in specific Course instances. Use name from Course unless the Course instance has a different name from the course. The name of the item." ;
            sh:path [ sh:alternativePath ( <http://schema.org/name> schema:name ) ] ],
        [ sh:description "An organizer of an Event." ;
            sh:path [ sh:alternativePath ( <http://schema.org/organizer> schema:organizer ) ] ],
        [ sh:description "For events within events e.g. guest lecture within a workshop event An Event that is part of this event. For example, a conference event includes many presentations, each of which is a subEvent of the conference. Supersedes subEvents. Inverse property: superEvent." ;
            sh:path [ sh:alternativePath ( <http://schema.org/subEvent> schema:subEvent ) ] ],
        [ sh:description "Use to describe the event a course instance takes place within. e.g. Galaxy Workshop during the ISMB Conference. An event that this event is a part of. For example, a collection of individual music performances might each have a music festival as their superEvent. Inverse property: subEvent." ;
            sh:path [ sh:alternativePath ( <http://schema.org/superEvent> schema:superEvent ) ] ],
        [ sh:description "A work featured in some event, e.g. exhibited in an ExhibitionEvent. Specific subproperties are available for workPerformed (e.g. a play), or a workPresented (a Movie at a ScreeningEvent). The training material used in this specific course instance, or produced for this course instance. If this is the same for all Course Instances, use hasPart and/or mentions in Course instead" ;
            sh:path [ sh:alternativePath ( <http://schema.org/workFeatured> schema:workFeatured ) ] ],
        [ sh:description "This is used to state the Bioschemas profile that the markup relates to. The identifier can be the url for the version of this bioschemas class on github: https://github.com/BioSchemas/specifications/blob/master/CourseInstance/jsonld/CourseInstance_v1.0-RELEASE.json" ;
            sh:minCount 1 ;
            sh:path dcterms:conformsTo ;
            sh:severity sh:Violation ] ;
