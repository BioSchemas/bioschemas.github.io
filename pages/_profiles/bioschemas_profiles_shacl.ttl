        [ sh:description "A relationship between an organization and a department of that organization, also described as an organization (allowing different urls, logos, opening hours). For example: a store with a pharmacy, or a bakery with a cafe." ;
            sh:path [ sh:alternativePath ( <http://schema.org/department> schema:department ) ] ],
        [ sh:description "The date that this organization was dissolved." ;
            sh:path [ sh:alternativePath ( <http://schema.org/dissolutionDate> schema:dissolutionDate ) ] ],
        [ sh:description "For an alliance or collaboration or project, the founding members. NOTE: schema/Organization has founder but this has a type of Person not Person and Organization" ;
