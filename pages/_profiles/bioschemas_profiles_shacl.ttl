<https://bioschemas.org/profiles/Person/0.3-DRAFT> a sh:NodeShape ;
    sh:description "A specification for describing people in life sciences The Life Science Person specification provides a way to describe bioscience events on the World Wide Web. It defines a set of metadata and vocabularies, built on top of existing technologies and standards, that can be used to represent the profile information of people in Web pages and applications. The goal of the specification is to make it easier to discover, life science profile information. Version: 0.3-DRAFT. <h3>Summary of Changes</h3><br>Changes since 0.2-DRAFT-2019_07_19:<ul><li>added sameAs property as recommended</li></ul>." ;
    sh:property [ sh:description "A description of the item." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/description> schema:description ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "This is used to state the Bioschemas profile that the markup relates to. The identifier can be the url for the version of this bioschemas class on github: https://github.com/BioSchemas/specifications/tree/master/Person/jsonld/Person_v0.3-DRAFT.json" ;
            sh:minCount 1 ;
            sh:path dcterms:conformsTo ;
            sh:severity sh:Violation ],
        [ sh:description "Email address." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/email> schema:email ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "A textual description of the topic which a person has the most expertise in. This should make use of an ontology such as the EDAM ontology" ;
            sh:minCount 1 ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/expertise> ;
            sh:severity sh:Warning ],
        [ sh:description "A contact location for a person's residence." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/homeLocation> schema:homeLocation ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "An image of the item. This can be a URL or a fully described ImageObject." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/image> schema:image ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "An Organization (or ProgramMembership) to which this Person or Organization belongs. Inverse property: member." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/memberOf> schema:memberOf ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "The url of ID of the person's ORCID (see http://orcid.org)" ;
            sh:minCount 1 ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/orcid> ;
            sh:severity sh:Warning ],
        [ sh:description "Organizations that the person works for." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/worksFor> schema:worksFor ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "URL of additional sources for the dataset" ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/sameAs> schema:sameAs ) ] ;
            sh:severity sh:Warning ],
        [ sh:description "A person who has been an educational, professional or personal advisor to the person." ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/advisor> ],
        [ sh:description "A link or body of text describing the person's notable achievements, employment history, and contact details." ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/curriculumVitae> ],
        [ sh:description "A textual description of the professional experiences a person has acquired." ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/experience> ],
        [ sh:description "Textual description of current or past funding ths person has acquired" ;
            sh:path [ sh:alternativePath ( <http://schema.org/funding> schema:funding ) ] ],
        [ sh:description "Any interests held by the person. These should be single words or short phrases that relate to a person" ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/interest> ],
        [ sh:description "Global connetions related to teaching and scholarly work in the past 3 years" ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/internatonalActivities> ],
        [ sh:description "Which journals this person is a reeree of" ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/journalReferee> ],
        [ sh:description "Indicates a page (or other CreativeWork) for which this thing is the main entity being described. See background notes for details. Inverse property: mainEntity." ;
            sh:minCount 1 ;
            sh:path [ sh:alternativePath ( <http://schema.org/mainEntityOfPage> schema:mainEntityOfPage ) ] ;
            sh:severity sh:Violation ],
        [ sh:description "Keywords to describe the person. These should be single words or short phrases that relate to a person. Keywords or tags used to describe this content. Multiple entries in a keywords list are typically delimited by commas." ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/keywords> ],
        [ sh:description "A list of languages spoken by the Person Of a Person, and less typically of an Organization, to indicate a known language. We do not distinguish skill levels or reading/writing/speaking/signing here. Use language codes from the IETF BCP 47 standard." ;
            sh:path [ sh:alternativePath ( <http://schema.org/knowsLanguage> schema:knowsLanguage ) ] ],
        [ sh:description "All or notable publications, presentations, training materials, research outputs or any other body of work attributed to this person." ;
            sh:path <https://discovery.biothings.io/view/bioschemasdrafts/works> ],
        [ sh:description "The name of the item." ;
