{
    "jsonld-context": {
        "schema": "http://schema.org/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "bioschemas": "https://discovery.biothings.io/view/bioschemas/"
    },
    "jsonld-graph": [
        {
            "jsonld-id": "bioschemas:Gene",
            "jsonld-type": "rdfs:Class",
            "jsonld-validation": {
                "type": "object",
                "properties": {
                    "identifier": {
                        "description": "The identifier property represents any kind of identifier for any kind of <a class=\"localLink\" href=\"http://schema.org/Thing\">Thing</a>, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See <a href=\"/docs/datamodel.html#identifierBg\">background notes</a> for more details.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            }
                        ]
                    },
                    "name": {
                        "description": "The name of the item.",
                        "type": "string"
                    },
                    "description": {
                        "description": "A description of the item.",
                        "type": "string"
                    },
                    "encodesBioChemEntity": {
                        "description": "For genes, this property is used to express in a generic way gene products encoded by this gene, for instance RNA or Protein",
                        "oneOf": [
                            {
                                "jsonld-ref": "#/definitions/bioChemEntity"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/bioChemEntity"
                                }
                            }
                        ]
                    },
                    "isPartOfBioChemEntity": {
                        "description": "Indicates a BioChemEntity that is (in some sense) a part of this BioChemEntity. Inverse property: hasBioChemEntityPart. For genes, it is recommended to at least specify the DNA/chromosome containing this gene.",
                        "oneOf": [
                            {
                                "jsonld-ref": "#/definitions/bioChemEntity"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/bioChemEntity"
                                }
                            }
                        ]
                    },
                    "url": {
                        "description": "URL of the item.",
                        "type": "string",
                        "format": "uri"
                    },
                    "alternateName": {
                        "description": "An alias for the item.",
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        ]
                    },
                    "alternativeOf": {
                        "description": "Is variant of. Use this property to express when a gene is a variant of any other gene.",
                        "oneOf": [
                            {
                                "jsonld-ref": "#/definitions/gene"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/gene"
                                }
                            }
                        ]
                    },
                    "associatedDisease": {
                        "description": "Disease associated to this BioChemEntity. Such a disease can be a MedicalCondition or a URL. If you want to add an evidence supporting the association, please use PropertyValue.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/medicalCondition"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/medicalCondition"
                                }
                            },
                            {
                                "jsonld-ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/propertyValue"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "expressedIn": {
                        "description": "Tissue, organ, biological sample, etc in which activity of this gene has been observed experimentally. For example brain, digestive system.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/definedTerm"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/definedTerm"
                                }
                            },
                            {
                                "jsonld-ref": "#/definitions/bioChemEntity"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/bioChemEntity"
                                }
                            },
                            {
                                "jsonld-ref": "#/definitions/anatomicalPart"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/anatomicalPart"
                                }
                            }
                        ]
                    },
                    "hasBioChemEntityPart": {
                        "description": "Indicates a BioChemEntity that (in some sense) has this BioChemEntity as a part. Inverse property: isPartOfBioChemEntity",
                        "oneOf": [
                            {
                                "jsonld-ref": "#/definitions/bioChemEntity"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/bioChemEntity"
                                }
                            }
                        ]
                    },
                    "hasBioPolymerSequence": {
                        "description": "Schema:  A symbolic representation of a BioChemEnity. For example, a nucleotide sequence of a Gene or an amino acid sequence of a Protein.",
                        "type": "string"
                    },
                    "hasMolecularFunction": {
                        "description": "Molecular function performed by this BioChemEntity; please use PropertyValue if you want to include any evidence.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/definedTerm"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/definedTerm"
                                }
                            },
                            {
                                "jsonld-ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/propertyValue"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "hasRepresentation": {
                        "description": "A common representation such as a protein sequence or chemical structure for this entity. For images use schema.org/image.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/propertyValue"
                                }
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "hasStatus": {
                        "description": "Schema:  One of pseudogene, dead, killed, live, predicted, suppressed.",
                        "type": "string",
                        "enum": [
                            "pseudogene",
                            "dead",
                            "killed",
                            "live",
                            "predicted",
                            "suppressed"
                        ]
                    },
                    "image": {
                        "description": "An image of the item. This can be a <a class=\"localLink\" href=\"http://schema.org/URL\">URL</a> or a fully described <a class=\"localLink\" href=\"http://schema.org/ImageObject\">ImageObject</a>.",
                        "type": "string",
                        "format": "uri"
                    },
                    "isInvolvedInBiologicalProcess": {
                        "description": "Biological process this BioChemEntity is involved in; please use PropertyValue if you want to include any evidence.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/definedTerm"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/definedTerm"
                                }
                            },
                            {
                                "jsonld-ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/propertyValue"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "isLocatedInSubcellularLocation": {
                        "description": "Subcellular location where this BioChemEntity is located; please use PropertyValue if you want to include any evidence.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/definedTerm"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/definedTerm"
                                }
                            },
                            {
                                "jsonld-ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/propertyValue"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "mainEntityOfPage": {
                        "description": "Indicates a page (or other CreativeWork) for which this thing is the main entity being described. See <a href=\"/docs/datamodel.html#mainEntityBackground\">background notes</a> for details.",
                        "type": "string",
                        "format": "uri"
                    },
                    "sameAs": {
                        "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
                        "type": "string",
                        "format": "uri"
                    },
                    "taxonomicRange": {
                        "description": "The taxonomic grouping of the organism that expresses, encodes, or in someway related to the BioChemEntity.",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/definedTerm"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/definedTerm"
                                }
                            },
                            {
                                "jsonld-ref": "#/definitions/taxon"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/taxon"
                                }
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    }
                },
                "required": [
                    "identifier",
                    "name"
                ],
                "recommended": [
                    "description",
                    "encodesBioChemEntity",
                    "isPartOfBioChemEntity",
                    "url"
                ],
                "optional": [
                    "alternateName",
                    "alternativeOf",
                    "associatedDisease",
                    "expressedIn",
                    "hasBioChemEntityPart",
                    "hasBioPolymerSequence",
                    "hasMolecularFunction",
                    "hasRepresentation",
                    "hasStatus",
                    "image",
                    "isInvolvedInBiologicalProcess",
                    "isLocatedInSubcellularLocation",
                    "mainEntityOfPage",
                    "sameAs",
                    "taxonomicRange"
                ],
                "definitions": {
                    "taxon": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "taxonRank": {
                                "anyOf": [
                                    {
                                        "jsonld-ref": "#/definitions/propertyValue"
                                    },
                                    {
                                        "type": "array",
                                        "items": {
                                            "jsonld-ref": "#/definitions/propertyValue"
                                        }
                                    },
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    },
                                    {
                                        "type": "string",
                                        "format": "uri"
                                    },
                                    {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "uri"
                                        }
                                    }
                                ]
                            }
                        },
                        "required": [
                            "name",
                            "taxonRank"
                        ],
                        "jsonld-type": "Taxon"
                    },
                    "propertyValue": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            }
                        },
                        "required": [],
                        "jsonld-type": "PropertyValue"
                    },
                    "definedTerm": {
                        "type": "object",
                        "properties": {
                            "url": {
                                "type": "string",
                                "format": "uri"
                            },
                            "name": {
                                "type": "string"
                            },
                            "termCode": {
                                "type": "string"
                            }
                        },
                        "required": [],
                        "jsonld-type": "DefinedTerm"
                    },
                    "medicalCondition": {
                        "type": "object",
                        "properties": {
                            "url": {
                                "type": "string",
                                "format": "uri"
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "required": [],
                        "jsonld-type": "MedicalCondition"
                    },
                    "bioChemEntity": {
                        "type": "object",
                        "properties": {
                            "url": {
                                "type": "string",
                                "format": "uri"
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "required": [],
                        "jsonld-type": "BioChemEntity"
                    },
                    "gene": {
                        "type": "object",
                        "properties": {
                            "identifier": {
                                "anyOf": [
                                    {
                                        "jsonld-ref": "#/definitions/propertyValue"
                                    },
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "string",
                                        "format": "uri"
                                    }
                                ]
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "required": [],
                        "jsonld-type": "Gene"
                    },
                    "quantitativeValue": {
                        "type": "object",
                        "properties": {
                            "value": {
                                "type": "string"
                            },
                            "unitText": {
                                "type": "string"
                            }
                        },
                        "required": [],
                        "jsonld-type": "QuantitativeValue"
                    },
                    "anatomicalPart": {
                        "anyOf": [
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    }
                                },
                                "required": [],
                                "jsonld-type": "AnatomicalStructure"
                            },
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    }
                                },
                                "required": [],
                                "jsonld-type": "AnatomicalSystem"
                            }
                        ]
                    }
                },
                "jsonld-schema": "http://json-schema.org/draft-07/schema#"
            },
            "rdfs-comment": "Bioschemas profile describing a Gene in Life Sciences. This Gene profile specification presents the markup for describing a Gene and is based on the 1.0 release",
            "rdfs-label": "Gene",
            "rdfs-subClassOf": {
                "jsonld-id": "bioschemas:BioChemEntity"
            }
        },
        {
            "schema:domainIncludes": {
                "jsonld-id": "bioschemas:Gene"
            },
            "schema:rangeIncludes": [
                {
                    "jsonld-id": "schema:Text"
                }
            ],
            "jsonld-id": "bioschemas:hasStatus",
            "jsonld-type": "rdf:Property",
            "rdfs-comment": "Schema:  One of pseudogene, dead, killed, live, predicted, suppressed.",
            "rdfs-label": "hasStatus"
        }
    ]
}