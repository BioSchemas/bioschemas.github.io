{
    "jsonld-context": {
        "schema": "http://schema.org/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "bioschemas": "https://discovery.biothings.io/view/bioschemas/"
    },
    "jsonld-graph": [
        {
            "jsonld-id": "bioschemas:Taxon",
            "jsonld-type": "rdfs:Class",
            "jsonld-validation": {
                "type": "object",
                "properties": {
                    "vernacularName": {
                        "description": "A vernacular (common) name of the taxon",
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        ]
                    },
                    "childTaxon": {
                        "description": "Closest child taxa of the taxon in question. Inverse property: parentTaxon",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/taxon"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/taxon"
                                }
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "parentTaxon": {
                        "description": "Closest parent taxon of the taxon in question. Inverse property: childTaxon",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/taxon"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/taxon"
                                }
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "taxonRank": {
                        "description": "The taxonomic rank of this taxon given preferably as a URI from a controlled vocabulary \u2013 (typically the ranks from TDWG TaxonRank ontology or equivalent Wikidata URIs).",
                        "anyOf": [
                            {
                                "jsonld-ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "jsonld-ref": "#/definitions/propertyValue"
                                }
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "additionalType": {
                        "description": "A Taxon type from a well known vocabulary, e.g. DarwinCore http://rs.tdwg.org/dwc/terms/Taxon or http://rs.tdwg.org/ontology/voc/TaxonConcept#TaxonConcept.",
                        "oneOf": [
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "alternateName": {
                        "description": "Scientific name, with authorship and date information if known, of a synonym of the currently valid (zoological) or accepted (botanical) name.",
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        ]
                    },
                    "name": {
                        "description": "Currently valid (zoological) or accepted (botanical) name for that taxon, with authorship and date information if known.",
                        "type": "string"
                    },
                    "sameAs": {
                        "description": "URL of third-party webpages describing the same taxon",
                        "oneOf": [
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ]
                    },
                    "url": {
                        "description": "URL of the item.",
                        "type": "string",
                        "format": "uri"
                    }
                },
                "required": [
                    "taxonRank",
                    "name"
                ],
                "recommended": [
                    "parentTaxon",
                    "url"
                ],
                "optional": [
                    "additionalType",
                    "alternateName",
                    "childTaxon",
                    "sameAs",
                    "vernacularName"
                ],
                "definitions": {
                    "taxon": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "taxonRank": {
                                "anyOf": [
                                    {
                                        "jsonld-ref": "#/definitions/propertyValue"
                                    },
                                    {
                                        "type": "array",
                                        "items": {
                                            "jsonld-ref": "#/definitions/propertyValue"
                                        }
                                    },
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    },
                                    {
                                        "type": "string",
                                        "format": "uri"
                                    },
                                    {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "uri"
                                        }
                                    }
                                ]
                            }
                        },
                        "required": [
                            "name",
                            "taxonRank"
                        ],
                        "jsonld-type": "bioschemas:Taxon"
                    },
                    "propertyValue": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            },
                            "identifier": {
                                "type": "string"
                            }
                        },
                        "required": [],
                        "jsonld-type": "PropertyValue"
                    }
                },
                "jsonld-schema": "http://json-schema.org/draft-07/schema#"
            },
            "rdfs-comment": "A set of organisms asserted to represent a natural cohesive biological unit.",
            "rdfs-label": "Taxon",
            "rdfs-subClassOf": {
                "jsonld-id": "schema:Thing"
            }
        },
        {
            "schema:domainIncludes": {
                "jsonld-id": "bioschemas:Taxon"
            },
            "schema:rangeIncludes": [
                {
                    "jsonld-id": "bioschemas:Taxon"
                },
                {
                    "jsonld-id": "schema:Text"
                },
                {
                    "jsonld-id": "schema:URL"
                }
            ],
            "jsonld-id": "bioschemas:childTaxon",
            "jsonld-type": "rdf:Property",
            "rdfs-comment": "Closest child taxa of the taxon in question. Inverse property: parentTaxon",
            "rdfs-label": "childTaxon"
        },
        {
            "schema:domainIncludes": {
                "jsonld-id": "bioschemas:Taxon"
            },
            "schema:rangeIncludes": [
                {
                    "jsonld-id": "bioschemas:Taxon"
                },
                {
                    "jsonld-id": "schema:Text"
                },
                {
                    "jsonld-id": "schema:URL"
                }
            ],
            "jsonld-id": "bioschemas:parentTaxon",
            "jsonld-type": "rdf:Property",
            "rdfs-comment": "Closest parent taxon of the taxon in question. Inverse property: childTaxon",
            "rdfs-label": "parentTaxon"
        },
        {
            "schema:domainIncludes": {
                "jsonld-id": "bioschemas:Taxon"
            },
            "schema:rangeIncludes": [
                {
                    "jsonld-id": "schema:PropertyValue"
                },
                {
                    "jsonld-id": "schema:Text"
                },
                {
                    "jsonld-id": "schema:URL"
                }
            ],
            "jsonld-id": "bioschemas:taxonRank",
            "jsonld-type": "rdf:Property",
            "rdfs-comment": "The taxonomic rank of this taxon given preferably as a URI from a controlled vocabulary \u2013 (typically the ranks from TDWG TaxonRank ontology or equivalent Wikidata URIs).",
            "rdfs-label": "taxonRank"
        },
        {
            "schema:domainIncludes": {
                "jsonld-id": "bioschemas:Taxon"
            },
            "schema:rangeIncludes": [
                {
                    "jsonld-id": "schema:Text"
                }
            ],
            "jsonld-id": "bioschemas:vernacularName",
            "jsonld-type": "rdf:Property",
            "rdfs-comment": "A vernacular (common) name of the taxon",
            "rdfs-label": "vernacularName"
        }
    ]
}